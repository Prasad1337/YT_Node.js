<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <ul>
      <% resultsData.forEach(function(item){ %>
        <li><a href="<%= item.link %>"><img src="<%= item.thumbnails.default.url %>"/></a></li>
      <% }); %>
    </ul>
  </body>
</html>
